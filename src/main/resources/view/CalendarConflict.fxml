<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox id="slotPane" fx:id="slotPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
        styleClass="calendar-grid-conflict">
  <VBox alignment="TOP_LEFT" prefHeight="50" GridPane.columnIndex="0">
    <HBox spacing="5" alignment="CENTER_LEFT">
      <Label fx:id="ids" styleClass="calendar_big_label" />
    </HBox>
    <Label fx:id="date" styleClass="calendar_small_label" text="\$date" />
    <Label fx:id="time" styleClass="calendar_small_label" text="\$time" />
    <Label fx:id="petName" styleClass="calendar_small_label" text="\$petName" />
  </VBox>
</HBox>
